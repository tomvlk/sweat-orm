{
  "name": "tomvlk/sweat-orm",
  "type": "library",
  "description": "Simple and sweat ORM for PHP, using your already existing structure, no command line required!",
  "homepage": "https://github.com/tomvlk/sweat-orm",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Valk",
      "email": "tomvalk@lt-box.info",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "doctrine/annotations": "1.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.1.3",
    "phpunit/php-code-coverage": "^3",
    "satooshi/php-coveralls": "0.7.*"
  },
  "autoload": {
    "psr-0": {
      "SweatORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "SweatORM\\Tests\\": "tests/SweatORM/Tests"
    }
  },
  "archive": {
    "exclude": [
      "!vendor",
      "tests",
      "*phpunit.xml",
      ".travis.yml",
      "composer.phar"
    ]
  }
}
